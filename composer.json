{
	"name": "wikibase/query",
	"type": "mediawiki-extension",
	"description": "Extension to Wikibase Repo that adds the ability to define and execute queries",
	"keywords": [
		"Wikibase",
		"WikibaseQuery",
		"query",
		"Wikidata"
	],
	"homepage": "https://github.com/wmde/WikibaseQuery",
	"license": "GPL-2.0+",
	"authors": [
		{
			"name": "Jeroen De Dauw",
			"email": "jeroendedauw@gmail.com",
			"homepage": "http://jeroendedauw.com",
			"role": "Developer"
		}
	],
	"support": {
		"issues": "https://bugzilla.wikimedia.org/",
		"irc": "irc://irc.freenode.net/wikidata"
	},
	"require": {
		"php": ">=5.3.0",
		"composer/installers": "1.*,>=1.0.1",
		"data-values/data-values": "~0.1",
		"data-values/interfaces": "~0.1",
		"ask/ask": "~1.0",
		"ask/serialization": "~1.0",
		"wikibase/data-model": ">=0.4",
		"wikibase/query-engine": "dev-master",
		"wikibase/wikibase": "dev-master",
		"serialization/serialization": "~2.0"
	},
	"autoload": {
		"files" : [
			"WikibaseQuery.php"
		],
		"psr-4": {
			"Wikibase\\Query\\": "src/"
		}
	}
}
